#CHECK IF THE NUMBER IS EVEN OR ODD!!!!!!!
'''num = int(input("Enter a number: "))
if (num % 2) == 0:
    print("{0} is Even number". format(num))
else:
    print("{0} is Odd number")'''
if __name__ == '__main__':
    n = int(input().strip())
if (n % 2) != 0:
    print("Weird")
if (n%2) == 0 and n in range(2,5):
    print("Not Weird")
if (n%2) == 0 and n in range(6,21):
    print("Weird")
if (n % 2) == 0 and n > 20:
    print("Not Weird")
